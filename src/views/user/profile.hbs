<div class="module-header">
    <h2 class="font-f-s font-24">Bienvenido(a) {{user.primer_nom}} {{user.primer_ape}}</h2>
    <div class="container">
        <h3 class="font-f-s font-24">UNESR</h3>
        <img src="/img/logo.png" alt="logo unesr" class="logo">
    </div>
</div>
<div class="profile-content p-10">
    <div class="profile-main">
        <div class="content-hide z-index-1000">
            <div class="popup">
                {{> edit-profile }}
            </div>
        </div>
    </div>
    <div class="profile-data">
        <section class="profile-img">
            <span class="container-img c-point" title="Cambiar Imagen">
                <img src="/img/user.png" alt="usuario">
                <p><i class="fas fa-edit"></i></p>
            </span>
        </section>
        <section class="profile-info font-12 uppercase">
            <aside class="font-14">{{user.primer_nom}} {{user.segundo_nom}} {{user.primer_ape}} {{user.segundo_ape}}</aside class="font-14">
            <aside><b>C.I:</b> {{user.id_usuario}}</aside>
            <aside><b>Cargo:</b> {{user.nombre_cargo}}</aside>
            <aside><b>Correo:</b> {{user.correo}}</aside>
            <aside><b>Tel√©fono:</b> 0{{user.numero}}</aside>
        </section>
        <section class="profile-button container">
            <button class="btn btn-blue-2 uppercase br-3 font-12"  id="profile-edit">Editar Perfil</button>
        </section>
    </div>
</div>
<script src="/js/profile.js"></script>