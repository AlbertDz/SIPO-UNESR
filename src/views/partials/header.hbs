<header class="header color-b-blue-3 z-index-1000">
    <section class="navigation">
        <aside class="container opt">
            {{> nav }}
        </aside>
        <aside class="container">
            <a class="no-link header-title container" href="/profile">
                <img src="/img/sipo-white.png" alt="SIPO">
            </a>
            {{> search }}
        </aside>
    </section>
    <section class="container option-header">
        <div class="fecha font-12 uppercase" id="fecha">
            {{ fecha }}
        </div>
        <aside class="btn-header container btn btn-blue-2 br-50">
            <span class="color-white container" title="Mas" id="btn-option-sesion"><i class="fas fa-ellipsis-v"></i></span>
            {{> option-session }}
        </aside>
    </section>
</header>
<script src="/js/header.js"></script>