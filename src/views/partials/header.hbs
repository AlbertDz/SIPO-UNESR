<header class="header color-b-blue-3 z-index-1000">
    <section class="navigation">
        <aside class="container opt">
            {{> nav }}
        </aside>
        <aside class="container">
            <a class="no-link header-title container color-b-blue-2" href="/profile">
                <img src="/img/sipo-white.png" alt="SIPO">
            </a>
        </aside>
        <aside class="container">
            <img src="/img/logo.png" alt="logo unesr" class="logo">
            <h3 class="font-f-s" id="add-user">UNESR</h3>
        </aside>
    </section>
    <section class="container option-header">
        <div class="fecha font-12 uppercase" id="fecha">
            {{ fecha }}
        </div>
        <aside class="btn-header btn btn-blue-2 br-50" id="btn-option-sesion">
            <span class="color-white container font-20" title="Mas">
                <i class="fas fa-user"></i>
            </span>
            {{> option-session }}
        </aside>
    </section>
</header>
<script src="/js/header.js"></script>